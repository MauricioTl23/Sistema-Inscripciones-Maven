<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane id="AnchorPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuOptionsController">
    <stylesheets>
        <URL value="@menuoptions.css" />
    </stylesheets>
   <children>
      <HBox spacing="25" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
         <children>
            <StackPane fx:id="stackPane1" prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="Rectangle1" arcHeight="25.0" arcWidth="25.0" fill="#529e62" height="600.0" stroke="BLACK" strokeType="INSIDE" width="440.0">
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin></Rectangle>
                  <VBox fx:id="vbins" prefHeight="0.0" prefWidth="0.0" spacing="10.0" StackPane.alignment="TOP_CENTER">
                     <children>
                        <Label text="INSCRIPCIONES" />
                        <ImageView fx:id="istudents" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnGestionarEstudiantes" mnemonicParsing="false" prefHeight="60.0" prefWidth="280.0" text="GESTIONAR ESTUDIANTES" />
                        <ImageView fx:id="inotes" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnGestionarNotas" mnemonicParsing="false" prefHeight="60.0" prefWidth="280.0" text="GESTIONAR NOTAS" />
                     </children>
                     <StackPane.margin>
                        <Insets bottom="25.0" left="10.0" right="10.0" top="25.0" />
                     </StackPane.margin>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </StackPane>
            <StackPane fx:id="stackPane2" prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="Rectangle2" arcHeight="25.0" arcWidth="25.0" fill="#529e62" height="600.0" stroke="BLACK" strokeType="INSIDE" width="440.0" />
                  <VBox fx:id="vbad" prefHeight="0.0" prefWidth="0.0" spacing="10.0" StackPane.alignment="TOP_CENTER">
                     <children>
                        <Label text="ADMINISTRATIVO" />
                        <ImageView fx:id="iusers" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnGestionarUsuarios" mnemonicParsing="false" prefHeight="60.0" prefWidth="280.0" text="GESTIONAR USUARIOS" />
                        <ImageView fx:id="isettings" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnConfiguracion" mnemonicParsing="false" prefHeight="60.0" prefWidth="280.0" text="GESTION ACADEMICA" />
                     </children>
                     <StackPane.margin>
                        <Insets bottom="25.0" left="10.0" right="10.0" top="25.0" />
                     </StackPane.margin>
                  </VBox>
               </children>
            </StackPane>
            <StackPane fx:id="stackPane3" prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="Rectangle3" arcHeight="25.0" arcWidth="25.0" fill="#529e62" height="600.0" stroke="BLACK" strokeType="INSIDE" width="440.0" />
                  <VBox fx:id="vbin" prefHeight="0.0" prefWidth="0.0" spacing="10.0" StackPane.alignment="TOP_CENTER">
                     <children>
                        <Label text="INFORMACION" />
                        <ImageView fx:id="ireports" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnReportes" mnemonicParsing="false" prefHeight="60.0" prefWidth="280.0" text="REPORTES" />
                        <ImageView fx:id="ihelp" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnAyuda" mnemonicParsing="false" prefHeight="60.0" prefWidth="280.0" text="AYUDA" />
                     </children>
                     <StackPane.margin>
                        <Insets bottom="25.0" left="10.0" right="10.0" top="25.0" />
                     </StackPane.margin>
                  </VBox>
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
